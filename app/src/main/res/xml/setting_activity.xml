<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <PreferenceCategory android:title="Settings" >
        <EditTextPreference
            android:title="Url here"
            android:summary="Insert Url Here"
            android:key="prefUrl"
            android:defaultValue="www.facebook.com"/>

        <EditTextPreference
            android:title="Frequency"
            android:summary="Set time between each update of quiz"
            android:key="prefFrequency"
            android:numeric="integer"
            android:defaultValue="1" />
    </PreferenceCategory>
</PreferenceScreen>
